 int main(void)
  {
  // Arrays of strings
string names[] = {"bob", "cade", "pain"};
string nums[] = {"+1-617-495-1000", "+1-617-495-1000", "+1-949-468-2750"};

 // Search for name
 string name = get_string("Name: ");
 for (int i = 0; i < 3; i++)
 {
 if (strcmp(names[i], name) == 0)
 {
 printf("Found %s\n", nums[i]);
 return 0;
 }
 }
 printf("Not found\n");
 return 1;
 }
